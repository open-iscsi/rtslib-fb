fabric ib_srpt {
    target %srpt_wwn {
        acl %str {
            mapped_lun %int {
                target_lun @(-3 lun)
                write_protect %bool(no)
            }
        }
        attribute {
            srp_max_rdma_size %int(65536)
            srp_max_rsp_size %int(256)
            srp_sq_size %int(4096)
        }
        lun %int backend %backend
    }
}
